<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assist.imobilandroidapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImobilAndroidApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.assist.imobilandroidapp.screens.client.main.ClientActivity"
            android:exported="false" />
        <activity
            android:name="com.assist.imobilandroidapp.screens.onboarding.login.LogInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screens.listing.ViewImagesActivity" />
        <activity
            android:name="com.assist.imobilandroidapp.screens.averageuser.main.AverageUserActivity"
            android:exported="false" />
        <activity
            android:name=".screens.onboarding.forgotpassword.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.assist.imobilandroidapp.screens.listing.ListingScreenActivity"
            android:exported="false" />
        <activity
            android:name="com.assist.imobilandroidapp.screens.onboarding.resetpassword.main.ResetPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.assist.imobilandroidapp.screens.main.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.assist.imobilandroidapp.screens.onboarding.singup.main.SignUpActivity"
            android:exported="false" />
    </application>

</manifest>